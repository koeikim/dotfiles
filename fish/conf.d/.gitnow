[ options ]

# Read text from system clipboard or from the standard input
# only when using Gitflow keybindings
clipboard = false

[ keybindings ]

# Alt + S
state = \eS

# Alt + Shift + E
stage = \eE

# Ctrl + Alt + E
unstage = \e\ce

# Alt + M
show = \em

# Alt + C
commit-all = \ec

# Alt + D
pull = \ed

# Alt + P
push = \ep

# Alt + U
upstream = \eu

# Alt + L
logs = \el

# Alt + F
feature = \ef

# Alt + H
hotfix = \eh
